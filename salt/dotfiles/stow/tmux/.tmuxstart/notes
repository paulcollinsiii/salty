setup_window() {
  send_keys $@ "cd ~/personal " "Enter"
  send_keys $@ "clear" "Enter"
}

new_session
new_window
new_window
tmux rename-window -t $session:0 Notes
tmux rename-window -t $session:1 notes-1
tmux rename-window -t $session:2 notes-2
setup_window notes-1
setup_window notes-2

send_keys Notes "cd ~/gdrive_personal/Notes/; vim ." "Enter"
