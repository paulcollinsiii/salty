setup_window() {
  tmux send-keys "cd ~/workspace/datanav" "Enter"
  tmux send-keys "workon di_py34" "Enter"
  tmux send-keys "clear" "Enter"
}

new_session
new_window
new_window
tmux rename-window -t $session:0 di-1
tmux rename-window -t $session:1 di-2
tmux rename-window -t $session:2 Notes

tmux select-window -t $session:di-1
setup_window
tmux send-keys "vim ." "Enter"

tmux select-window -t $session:Notes
setup_window
tmux send-keys "cd ~/Google\ Drive/Notes/brightscope; vim ." "Enter"

tmux select-window -t $session:di-2
setup_window
