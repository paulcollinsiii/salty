setup_window() {
  send_keys $@ "workon bright" "Enter"
  send_keys $@ "clear" "Enter"
}

new_session
new_window
new_window
tmux rename-window -t $session:0 bright-1
tmux rename-window -t $session:1 bright-2
tmux rename-window -t $session:2 Notes
setup_window bright-1
setup_window bright-2

send_keys di-1 "vim ." "Enter"
send_keys Notes "cd ~/gdrive_personal/Notes/brightscope; vim ." "Enter"
