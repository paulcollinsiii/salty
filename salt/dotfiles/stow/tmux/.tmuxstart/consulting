setup_window() {
  send_keys $@ "cd ~/consulting " "Enter"
  send_keys $@ "clear" "Enter"
}

new_session
new_window
new_window
tmux rename-window -t $session:0 consulting-1
tmux rename-window -t $session:1 consulting-2
tmux rename-window -t $session:2 Notes
setup_window consulting-1
setup_window consulting-2

send_keys Notes "cd ~/gdrive_personal/Notes/; vim ." "Enter"
